package business.entity.medicine;

public class Medicine {
	/**ID*/
	private java.lang.String id;
	/**药品名称*/
	private java.lang.String name;
	/**药品单价*/
	private float price;
	/**计费方式：1.片；2.ml*/
	private java.lang.Integer methods;
	/**药品是否有效：0.无效；1.有效*/
	private java.lang.Integer isEnabled;
	/**药品信息上次更新时间*/
	private java.lang.String updateTime;
	/**药品规格：片/盒；ml/盒*/
	private java.lang.Integer norms;
	/**库存量*/
	private java.lang.Integer num;
	/**散装库存量*/
	private float specNum;
	/**备注*/
	private java.lang.String comm;

}
